#rofi.width: 40
#rofi.lines: 5
#rofi.columns: 1
#rofi.font: NotoMono Nerd Font 11
#rofi.bw: 0
#rofi.padding:0
#rofi.eh: 1
#rofi.separator-style: solid
#rofi.hide-scrollbar: true

